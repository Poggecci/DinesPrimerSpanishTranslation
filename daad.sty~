    \dindex{#1!#2!#3}%
    \dindex{#1!#3!#2}%
    \dindex{#2!#1!#3}%
    \dindex{#2!#3!#1}%
    \dindex{#3!#1!#2}%
    \dindex{#3!#2!#1}}

