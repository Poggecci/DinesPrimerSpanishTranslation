\newcommand{\nyeksframe}[3]{%
\begin{primerexample}\hspace*{-2mm}. {\lilacolor{#2.}}\label{#2}\label{#1}%
\eindex{#1 (\#\,\protect\ref{#2})|(}%
\eindex{\protect\ref{#1} #2|(}%
{\rm #3 \hfill\dbsquare}%\vspace*{-2mm}
\eindex{\protect\ref{#1} #2|)}%
\eindex{#1 (\#\,\protect\ref{#2})|)}%
\end{primerexample}
}

\newcommand{\nyeksframeu}[3]{%
\begin{primerexample}\hspace*{-2mm}. {\lilacolor{#2.}}\label{#2}\label{#1}%
\eindex{#1 (\#\,\protect\ref{#2})|(}%
\eindex{\protect\ref{#1} #2|(}%
{\rm #3}%\vspace*{-2mm}
\eindex{\protect\ref{#1} #2|)}%
\eindex{#1 (\#\,\protect\ref{#2})|)}%
\end{primerexample}
}

