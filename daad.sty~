\newcommand{\ileksframe}[2]{%
\begin{example}\hspace*{-2mm}. {\lilacolor{#1.}}\label{#1}%
\addcontentsline{toc}{subparagraph}{\lilacolor{Example \ref{#1}: #1}}
\eindex{#1 (\#\,\protect\ref{#1})|(}%
\eindex{\protect\ref{#1} #1|(}%
{\rm #2}%\vspace*{-2mm}
\eindex{\protect\ref{#1} #1|)}%
\eindex{#1 (\#\,\protect\ref{#1})|)}%
\end{example}
}

