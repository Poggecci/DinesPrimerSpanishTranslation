\newcommand{\starteks}[2]%
{\begin{primerexample}\label{#1}\dbeat{\sort{Example
      \arabic{primerexample}. }}\bbcolor{\textbf{#2:}}\sl\
      \dbeat{\stepcounter{primerexample}}}  
  
\renewcommand{\starteks}[2]{\vspace*{2mm}%
\boiteepaisseavecuntitre{\sort{Example \arabic{primerexample}:} \brcolor{#1}}
\vspace*{-3mm}\index{eind}{#1}\stepcounter{primerexample}
\noindent\footnotesize\sf{#2}
\endboiteepaisseavecuntitre\normalsize\rm
\vspace*{2mm}
}
