    \cindex{#1!#2}%
    \cindex{#2!#1}}
\renewcommand{\termiii}[3]{\textsl{#1 #2 #3}%
    \cindex{#1!#2!#3}%
    \cindex{#1!#3!#2}%
    \cindex{#2!#1!#3}%
    \cindex{#2!#3!#1}%
    \cindex{#3!#2!#1}%
    \cindex{#3!#1!#2}} 

