@Article{Ropke95a,
  author = 	 "B. Ropke and W. Seibel and A. Wozniak",
  title = 	 "{Verification and function test of electronic 
interlockings in the system test centre 
(rail traffic control)}",
  journal = 	 "{Signal und Draht}",
  year = 	 "1995",
  OPTkey = 	 "",
  volume = 	 "87",
  number = 	 "4",
  OPTmonth = 	 "",
  pages = 	 "120-2",
  note = 	 "\dbabstract{In the system 
test centre described, railway 
electronic interlockings are comprehensively 
tested and verified. Until now, this could only be
 done on site, at the original system for the customer. 
Configuration of the test centre allows simulation 
of every route system in service which is available 
as real-time simulation. In the system test centre, 
the system checks for new installations as well as for
 reconstructions, internal acceptances and fault 
processing are carried out. System checks consist 
of a function integrity test, detection of
 time response and interface and stress tests. 
By this procedure, faults in the software can 
be detected and corrected before delivery of the
 interlocking. The quality of the product is 
increased and additional costs arising for 
the concerned railway authority at a later 
stage due to limited availability are prevented. 
When interlockings are reconstructed, the new 
data can be prechecked so that the change on 
site can be realized much faster than until now. 
Consequently, operational inconveniences can 
be reduced to a minimum. \dbkey{% 
computer aided software engineering; digital 
simulation; program testing; program verification; 
rail traffic; railways; real-time systems;
 software reliability; traffic control; 
traffic engineering computing}} \rep",
  OPTannote = 	 "in German"
}

