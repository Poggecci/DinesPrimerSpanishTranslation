@InProceedings{Mortimer94,
  author =       "Rudolf G. Mortimer",
  title =        "Oh! Say, Can You Hear that Train Coming to the
                 Crossing?",
  booktitle =    "Proceedings of the Human Factors and Ergonomics
                 Society 38th Annual Meeting",
  series =       "SAFETY: Safety Potpourri II [Lecture]",
  volume =       "2",
  pages =        "898--902",
  year =         "1994",
  copyright =    "(c) Copyright 1994 Human Factors and Ergonomics
                 Society",
  mrnumber =     "C.HFS.94.898",
  note =     "\dbabstract{WARNING: 
THE ABSTRACT OF THIS ENTRY HAS NOT
                 BEEN VALIDATED. How effective is the horn on trains in alerting
                 motorists, cyclists and pedestrians of the approach of
                 a train to a highway-railroad crossing? Road users
                 become aware of trains approaching crossings either by
                 seeing them, hearing the warning horn or by warning
                 devices at the crossing. Auditory warnings have some
                 advantages over others, but practical problems of sound
                 transmission and the noise environment of road users
                 limit their effectiveness. Factors that affect the
                 audibility of train horns and crossing bells are
                 discussed. Train and crossing bells are generally
                 effective for persons in their immediate environment,
                 but are relatively ineffective for occupants of closed
                 vehicles. Because of the lack of reliability of the
                 detectability of auditory warning signals provided to
                 motorists and others approaching rail-highway
                 crossings, they should visually scan the tracks to
                 ascertain the presence of a train}"
}

@Article{profeta96a,
  key =          "profeta96a",
  author =       "J. A. Profeta III and N. P. Andrianos and Bing Yu and
                 B. W. Johnson and T. A. DeLong and D. Guaspart and D.
                 Jamsck",
  title =        "Safety-critical systems built with {COTS}",
  journal =      "IEEE Computer",
  month =        nov,
  year =         "1996",
  volume =       "29",
  number =       "11",
  pages =        "54--60",
  note =     "\dbabstract{In the rail transportation industry competitive
                 pressure has led to the increased use of COTS
                 (commercial off-the-shelf equipment in safety critical
                 systems), making it imperative that we extend proven
                 safety techniques to COTS based systems as well. To
                 this end, we have developed the Vital Framework
                 (V-Frame), which is used to develop a safety critical
                 platform from COTS hardware and software. The key
                 technologies in this framework are formal methods,
                 information redundancy, a proprietary data format, and
                 a concurrent checking scheme. Combining these
                 technologies results in a real time, checkable
                 correctness criterion that is a signature of the
                 application's algorithm structure and is independent of
                 both the hardware and the operating system. V-Frame's
                 most significant attribute is that the fail safe
                 properties of applications do not require the firmware
                 to be correct: the application will operate in a fail
                 safe (or vital) manner even if there are design faults
                 in the operating system and/or the hardware fails. This
                 does not mean that the application does not have to be
                 correctly specified and designed. Formal methods are
                 appropriate in the design of safety critical COTS
                 systems because a generic processing environment is
                 analogous to a formal system: it is designed to apply
                 well defined transformation rules to inputs (7
		  Refs.)}"
}


@InProceedings{Lewis95,
  author =       "Benjamin G. Lewis and James P. Cowan",
  title =        "A holistic approach to modeling outdoor acoustical
                 environments",
  booktitle =    "Proceedings of the Eighth Annual Symposium on
                 Geographic Information Systems for Transportation
                 (GIS-T)",
  pages =        "435--440",
  address =      "Sparks, Nevada",
  year =         "1995",
  note =     "\dbabstract{The dominant noise sources in our current outdoor
                 environment are typically related to highway vehicles,
                 aircraft, construction, and rail and industrial
                 operations. Typical environmental analyses examine each
                 of these sources independently without combining them
                 into one model that would predict the complete
                 acoustical environment in a specific geographic area.
                 Without the capability to combine all potential noise
                 sources, a realistic prediction of a noise environment
                 cannot be achieved. A GIS model has been developed that
                 considers all dominant noise sources and propagation
                 effects to yield contour mapping of a predicted
                 environment including all of these sources
                 simultaneously. The capability also exists to separate
                 each component to view the contribution that each noise
                 source has toward the complete acoustic environment.
                 This model uses standard methodologies prescribed by
                 the Federal Highway Administration, the Federal
                 Aviation Administration, the Federal Transit
                 Administration, and the Society of Automotive
                 Engineers, combined with empirical methodologies
                 associated with them}"
}

@InProceedings{Sirota95,
  author =       "Stuart Sirota and Victor Henry",
  title =        "Using {GIS} to identify locations with the greatest
                 potential increased light rail ridership",
  booktitle =    "Proceedings of the Eighth Annual Symposium on
                 Geographic Information Systems for Transportation
                 (GIS-T)",
  pages =        "371--382",
  address =      "Sparks, Nevada",
  year =         "1995",
  note =     "\dbabstract{This paper represents how GIS was used in support of a
                 marketing initiative to increase ridership on the
                 Central Light Rail Line in Baltimore, Maryland. A
                 primary objective of the initiative was to develop a
                 marketing strategy which identified geographic
                 locations in the region in which to target marketing
                 efforts. GIS was identified as an ideal tool to perform
                 spatial analysis to yield the desired results. The
                 analysis was performed by importing several existing
                 data sources into the GIS. These data sources included
                 passenger survey data, census data, and local
                 demographic forecasts. Using GIS, it was possible to
                 ultimately isolate individual transportation analysis
                 zones which contained the greatest potential for new
                 ridership. GIS also created dramatic color thematic
                 maps used to present the findings of the study to
                 senior management}"}

                                    

