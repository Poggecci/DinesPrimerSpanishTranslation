@TechReport{amore:aldwkw99a,
  author = 	 "Annegret Liebers and Dorothea Wagner 
                  and Karsten Weihe",
  title = 	 "{Recognizing Bundles in Time Table Graphs --- 
                   A Structural Approach}",
  institution =  konstanz,
  year = 	 "1999",
  OPTkey = 	 "",
  type = 	 "Research Report",
  OPTnumber = 	 "",
  OPTaddress = 	 "",
  OPTmonth = 	 "June 17--19",
  note = 	 "\dbabstract{\label{amore:aldwkw99a}
Based on a time table that
                   records the train journies of many trains
                   over a rail net of very many stations
                   and with very many paths linking many of
                   these stations, the problem is to
                   [re--]construct the net. Some train
                   journies travel between sequences of two
                   or more neighbouring stations. Other train
                   journies bypass some such neighbouring 
                   stations. The problem is, from the time
                   table only to recognise which are in fact
                   neighbouring stations and which trains are 
                   in fact bypassing some such stations --- and
                   which are not, that is: traveling along
                   ``adjacent'' (``parallel'') paths. Stations
                   are modeled as nodes of a graph. Edges of
                   the graph designate non--stop train trips
                   between pairs of stations. A station
                   from which several paths of length one or 
                   more lead along different rail lines to 
                   different or same (next) stations induce
                   a partition of the edges into
                   \textsl{bundles}. The paper describes
                   a modeling process for the classification
                   of bundles.}",
  OPTannote = 	 ""
}

@Article{amore:aldwkw99b,
  author = 	 "Annegret Liebers and Dorothea Wagner 
                  and Karsten Weihe",
  title = 	 "{On the Hardness of Recognising Bundles
                   in Time Table Graphs}",
  journal = 	 "International Journal of Foundations of
                  Computer Science, World Scientific 
                  Publishing Company, Singapore",
  year = 	 "1999 (or 2000)",
  OPTkey = 	 "",
  OPTvolume = 	 "",
  OPTnumber = 	 "",
  OPTmonth = 	 "",
  pages = 	 "13 pages",
  note = 	 "\dbabstract{\label{amore:aldwkw99b}
From a time table which
                  records the train journies of many trains
                  over an unknown rail net the problem to
                  be solved using the techniques of this 
                  paper is that of constructing a rail net
                  graph where nodes designate railway stations
                  and where edges designate rail lines between
                  stations. The problem is tricky in that some
                  train journies may emanate from a station
                  to different stations but along the same
                  linear sequence of lines (ie. 
                  \textsl{bundle}), whereas other
                  train journies may emanate from the same 
                  station to some of the same other stations 
                  but along the different
                  linear sequences of lines (ie. different
                  \textsl{bundles}). The recognition problem 
                  is from NP hard to NP complete.}",
  OPTannote = 	 ""}

