@techreport{db95:UNUIIST:37,
AUTHOR       = "Dines Bj{\o}rner and C.W. George and S. Prehn",
TITLE        = "{Domain Analysis --- 
a Prerequisite for Requirements Capture}",
INSTITUTION  = "{\sf UNU/IIST}",
ORGANIZATION = "International Institute for Software
                Technology, United Nations University",
ADDRESS      = "P.O.Box 3058, Macau",
YEAR         = "1995",
MONTH        = "February",
TYPE         = "Technical Report",
NUMBER       = "37",
NOTE         = "\dbabstract{Before an 
architecture for a software
		  system (or package) - that is: a definition of
		  concepts and facilities of the software system
		  (resp.\ package) 
to be developed - can be given, one
		  first 
establishes proper requirements expected 
of that software, that is: we
		  base software development on requirements
		  specifications. 
These requirements are captured from
		  understanding first the domain in 
which the software is to serve and then from understanding the
		  expectations of 
the new software as an operational
		  part of the thus 
extended domain. Thus understanding
		  the domain seems to be a very first 
order of `business'! In this paper we present an example of the
		  collection of 
(informal) procedures and (formal)
		  techniques that 
are being applied in understanding
		  the Railway System domain - as part of a 
large scale technology transfer project 
joint with various software
		  development and 
computer centres of the Chinese
		  Railways in researching and developing proper
		  software eventually for all aspects of 
railway operation in China}"}

@techreport{db96:r:UNUIIST:60,
AUTHOR       = "Dines Bj{\o}rner",
TITLE        = "{Models of Enterprise Management:
   Strategy, Tactics \& Operations
   --- Case Study Applied to Airlines and Manufacturing}",
INSTITUTION  = "{\sf UNU/IIST}",
ORGANISATION = "International Institute for Software
                  Technology, United Nations University",
TYPE         = "Technical Report",
NUMBER       = "60",
ADDRESS      = "P.O.Box 3058, Macau",
MONTH        = "January -- April",
YEAR         = "1996",
NOTE         = "\dbabstract{Based on an
 abstract notion of resources,
		  their macro allocation 
and macro scheduling, we give
		  models of strategic, 
tactical and operational
		  planning as well as 
of the actual overall operations
		  of a 
typical transportation or manufacturing 
enterprise. We comment on
		  macro- vs. micro 
allocation and scheduling. The
		  former binds resources 
to locations and time
		  intervals, the latter 
binds tasks to (already 
location allocated and interval scheduled) resources. 
The abstract and the instantiated models 
are claimed to represent
		  views of classes of 
application domains and to
		  constitute essential 
elements of an analysis of such
		  a domain}"} 


    
